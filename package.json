{
  "name": "smart-gym",
  "version": "0.1.0",
  "description": "Demo of a gym assistant app",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "@tensorflow/tfjs": "^4.4.0",
    "@tensorflow/tfjs-backend-wasm": "^4.4.0",
    "parcel-reporter-static-files-copy": "^1.5.0"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development parcel index.html --no-hmr --open",
    "build": "cross-env NODE_ENV=production parcel build --no-scope-hoist index.html --public-url ./",
    "link-local": "yalc link"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "clang-format": "~1.2.2",
    "cross-env": "^5.1.6",
    "parcel": "~2.3.2",
    "process": "^0.11.10",
    "yalc": "~1.0.0-pre.53"
  },
  "resolutions": {
    "node-fetch": "2.6.7",
    "ansi-regex": "5.0.1"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "ios >= 10",
    "android >= 6"
  ],
  "staticFiles": [
    {
      "staticPath": "./node_modules/@tensorflow/tfjs-backend-wasm/dist/tfjs-backend-wasm.wasm"
    },
    {
      "staticPath": "./node_modules/@tensorflow/tfjs-backend-wasm/dist/tfjs-backend-wasm-threaded-simd.wasm"
    },
    {
      "staticPath": "./node_modules/@tensorflow/tfjs-backend-wasm/dist/tfjs-backend-wasm-simd.wasm"
    }
  ]
}
